<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd" shortDescription="File Excel">
	 <fieldProperty
		name="nome_file"
		property="nome_file"
		maxLength="400"
		inputSize="40"
		inputType="TEXT"
		label="Nome del File"
		caseSensitiveSearch="false"
		enabledOnSearch="false"
		enabledOnEdit="false"		
		enabledOnInsert="false" />
	<fieldProperty
		name="ds_file"
		property="ds_file"
		maxLength="2000"
		inputSize="2000"
		inputType="TEXTAREA"
		cols="80"
		rows="3"
		label="Descrizione"
		caseSensitiveSearch="false"/>
	<fieldProperty
		name="blob"
		property="blob"
		maxLength="400"
		inputSize="40"
		inputType="FILE"
		label="File"
		caseSensitiveSearch="false"
		nullable="false" />
	<fieldProperty
		name="stato"
		property="stato"
		inputType="RADIOGROUP"
		layout="HORIZONTAL"
		keysProperty="statoKeys"
		readonlyProperty="ROExcel_blobManuale"
		label="Stato"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="tipo"
		property="tipo"
		inputType="RADIOGROUP"
		layout="HORIZONTAL"
		keysProperty="tipoKeys"
		readonlyProperty="ROExcel_blobManuale"
		label="Tipo"
		caseSensitiveSearch="false" />
		
	<fieldProperty
		name="attivaExcel_blob"
		property="nome_file"
		inputType="BUTTON"
		img="img/import24.gif"
		label="Attiva File"
		href="javascript:doScarica()" 
		readonlyProperty="ROAttivaExcelBlob" />		

	<fieldProperty
		name="dacr"
		property="dacr"
		inputType="ROTEXT"
		label="Data accodamento"
		formatName="timestamp" />
	<fieldProperty
		name="duva"
		property="duva"
		inputType="ROTEXT"
		label="Data esecuzione"
		formatName="timestamp" />
		
	 <formFieldProperty
		name="nome_file"
		label="Nome del File" />
	 <formFieldProperty
		name="blob"
		label="File" />		
	 <formFieldProperty
		name="ds_file"
		label="Descrizione del File" />
	 <formFieldProperty
		name="attivaExcel_blob"/>		
	 <formFieldProperty
		name="stato"/>
	 <formFieldProperty
		name="tipo"/>	
	 <formFieldProperty
		name="dacr"/>	
	 <formFieldProperty
		name="duva"/>	

	<columnFieldProperty
		name="dacr"/>		
	<columnFieldProperty
		name="nome_file" />
	<columnFieldProperty
		name="ds_file" />
	<columnFieldProperty
		name="duva"/>		
	<columnFieldProperty
		name="stato"/>
	<columnFieldProperty
		name="tipo"/>
	
	<findFieldProperty
		name="nome_file" />
	<findFieldProperty
		name="ds_file" />
	<findFieldProperty
		name="stato"/>
	<findFieldProperty
		name="tipo"/>
	<findFieldProperty
		name="dacr"/>
	<findFieldProperty
		name="duva"/>
		
</bulkInfo>