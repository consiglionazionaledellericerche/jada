<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="fl_dir"
		property="fl_dir"
		inputType="CHECKBOX"
		label="fl_dir" />
	<fieldProperty
		name="itemname"
		property="itemname"
		maxLength="50"
		inputSize="50"
		inputType="TEXT"
		label="Nome"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="ds_file"
		property="ds_file"
		maxLength="300"
		inputSize="300"
		inputType="TEXTAREA"
		label="Descrizione" />
	<fieldProperty
		name="ds_utente"
		property="ds_utente"
		maxLength="300"
		inputSize="300"
		inputType="TEXTAREA"
		label="Descr. utente" />

	<columnFieldProperty
		name="itemname" />
	<columnFieldProperty
		name="ds_file" />
	<columnFieldProperty
		name="ds_utente" />
	<columnFieldProperty
		name="stato" />
	<columnFieldProperty
		name="dacr" />
	<columnFieldProperty
		name="utcr" />
